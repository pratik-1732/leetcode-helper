{
  "manifest_version": 3,
  "name": "LeetCode AI Helper",
  "version": "1.0",
  "description": "Generate step-by-step solution and code for LeetCode problems using Gemini AI.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://leetcode.com/*",
    "https://generativelanguage.googleapis.com/*"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
